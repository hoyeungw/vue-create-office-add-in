{
  "name": "vue-create-office-add-in",
  "version": "0.0.1",
  "description": "An excel web add-in created via @vue/cli for test",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "^4.3.95",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.2.1",
    "roboto-fontface": "^0.10.0",
    "veho": "^0.1.7",
    "vue": "^2.6.10",
    "vuetify": "^2.0.15",
    "xbrief": "^0.1.24"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-plugin-vue": "^5.2.3",
    "sass": "^1.22.10",
    "sass-loader": "^7.3.1",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoyeungw/vue-create-office-add-in.git"
  },
  "keywords": [
    "vue",
    "@vue/cli",
    "excel",
    "office",
    "analytics"
  ],
  "author": "Hoyeung Wong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hoyeungw/vue-create-office-add-in"
  },
  "homepage": "https://github.com/hoyeungw/vue-create-office-add-in#readme"
}
